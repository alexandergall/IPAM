datatypes xsd = "http://www.w3.org/2001/XMLSchema-datatypes"

start = alternatives

alternatives = element alternatives {
   attribute xml:base { text }?,
   element alternative {
      attribute label { text },
      attribute state { text },
      element allowed-state { text }+
   }*
}
